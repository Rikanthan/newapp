<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/blue"
    android:orientation="vertical">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="269dp"
        android:orientation="vertical"></LinearLayout>

    <Button
        android:id="@+id/button"
        android:layout_width="113dp"
        android:layout_height="50dp"
        android:layout_gravity="center"

        android:background="@drawable/button"
        android:forceDarkAllowed="true"

        android:foregroundGravity="center"
        android:shadowColor="#A8A8A8"
        android:shadowDx="0"
        android:shadowDy="0"
        android:shadowRadius="5"
        android:text="Year 1 semi 1"
        android:textColor="#827717" />

    <Button
        android:id="@+id/button2"
        android:layout_width="113dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:background="@drawable/button"
        android:foregroundGravity="center"
        android:textColor="#827717"
        android:onClick="y1s2"
        android:shadowColor="#A8A8A8"
        android:shadowDx="0"
        android:shadowDy="0"
        android:shadowRadius="5"
        android:text="YEAR 1 SEMI 2" />

    <Button
        android:id="@+id/button6"
        android:layout_width="112dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:textColor="#827717"
        android:background="@drawable/button"
        android:foregroundGravity="center"
        android:shadowColor="#A8A8A8"
        android:shadowDx="0"
        android:shadowDy="0"
        android:shadowRadius="5"
        android:text="YEAR 2 SEMI 1" />

    <Button
        android:id="@+id/button7"
        android:layout_width="113dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:background="@drawable/button"
        android:foregroundGravity="center"
        android:textColor="#827717"
        android:shadowColor="#A8A8A8"
        android:shadowDx="0"
        android:shadowDy="0"
        android:shadowRadius="5"
        android:text="YEAR 2 SEMI 2" />

    <Button
        android:id="@+id/button8"
        android:layout_width="112dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:background="@drawable/button"
        android:textColor="#827717"
        android:foregroundGravity="center"
        android:onClick="y3s1"
        android:shadowColor="#A8A8A8"
        android:shadowDx="0"
        android:shadowDy="0"
        android:shadowRadius="5"
        android:text="YEAR 3 SEMI 1" />

    <Button
        android:id="@+id/button9"
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:background="@drawable/button"
        android:foregroundGravity="center"
        android:shadowColor="#A8A8A8"
        android:textColor="#827717"
        android:shadowDx="0"
        android:shadowDy="0"
        android:shadowRadius="5"
        android:text="YEAR 3 SEMI 2" />

</LinearLayout>