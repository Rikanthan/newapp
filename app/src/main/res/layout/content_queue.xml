<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/newbac"
    android:orientation="vertical"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".queue"
    tools:showIn="@layout/activity_queue">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="162dp"
        android:orientation="vertical"></LinearLayout>

    <Button
        android:id="@+id/button31"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="queue"
        android:text="queue"
        android:textColor="#F5F4F0"
        android:background="@drawable/button3"/>

    <Button
        android:id="@+id/button37"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="testq"
        android:text="testqueue"
        android:textColor="#F5F4F0"
        android:background="@drawable/button3"/>

    <Button
        android:id="@+id/button36"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="pal"
        android:text="palindrome"
        android:textColor="#F5F4F0"
        android:background="@drawable/button3"/>

    <Button
        android:id="@+id/button38"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="cqu"
        android:text="circle queue"
        android:textColor="#F5F4F0"
        android:background="@drawable/button3"/>

</LinearLayout>